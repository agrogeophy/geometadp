   "source": [
    "import ipywidgets as widgets\n",
    "\n",
    "box = widgets.Checkbox(False, description='checker')\n",
    "out = widgets.Output()\n",
    "\n",
    "@out.capture()\n",
    "def changed(b):\n",
    "    print(b)\n",
    "\n",
    "box.observe(changed)\n",
    "\n",
    "display(box)\n",
    "display(out)"
   ]
